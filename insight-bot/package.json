{
  "name": "insight-bot",
  "version": "1.0.0",
  "description": "Consciousness-serving Reddit companion with PLK v5.0 - Built by GestaltView",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Keith Soyka <keith@gestaltview.com>",
  "keywords": [
    "reddit",
    "devvit",
    "bot",
    "consciousness",
    "plk",
    "mental-health",
    "gestaltview"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "build": "vite build",
    "build:client": "cd src/client && vite build",
    "build:server": "cd src/server && vite build",
    "dev": "devvit playtest",
    "dev:client": "cd src/client && vite build --watch",
    "dev:devvit": "devvit playtest",
    "dev:server": "cd src/server && vite build --watch",
    "dev:vite": "cd src/client && vite --port 7474",
    "dev:watch": "vite build --watch",
    "check": "npm run type-check && npm run lint:fix && npm run prettier",
    "type-check": "tsc --build",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prettier": "prettier-package-json --write ./package.json && prettier --write .",
    "login": "devvit login",
    "deploy": "npm run build && devvit upload",
    "launch": "npm run build && devvit upload && devvit publish",
    "logs": "devvit logs insight-bot",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist node_modules .devvit",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "devvit": "^0.12.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "express": "^5.1.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tailwindcss/vite": "^4.1.6",
    "@types/express": "^5.0.1",
    "@types/node": "^22.10.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "prettier-package-json": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.4.1",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}